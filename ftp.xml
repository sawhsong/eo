<?xml version="1.0" encoding="UTF-8"?>
<project name="ZZZ - Copy war to Server" default="ftp" basedir=".">
	<property file="ftp.properties"/>

	<target name="ftp" description="upload through ftp">
		<scp todir="${server.id}:${server.pw}@${server.ip}:${server.dir.root}/${server.dir.buildset}"
			file="${src.war}/${war.fileName}"
			trust="true"
		/>
	</target>
</project>