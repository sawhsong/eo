<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN" "http://struts.apache.org/dtds/struts-2.5.dtd">
<struts>
	<package name="login" extends="default" namespace="/" strict-method-invocation="false">
		<default-interceptor-ref name="ZebraDefaultStack"/>
		<action name="login/*" class="com.es.portal.login.LoginAction" method="{1}">
			<result name="loginPage" type="debugDispatcherResult">/com/es/portal/login/login.jsp</result>
			<result name="resetPassword" type="debugDispatcherResult">/com/es/portal/login/resetPasswordPop.jsp</result>
			<result name="requestRegister" type="debugDispatcherResult">/com/es/portal/login/requestRegisterPop.jsp</result>
			<result name="userProfile" type="debugDispatcherResult">/com/es/portal/login/userProfilePop.jsp</result>
			<result name="updateUserProfile" type="debugDispatcherResult">/com/es/portal/login/updateUserProfilePop.jsp</result>
		</action>
	</package>
</struts>