<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN" "http://struts.apache.org/dtds/struts-2.5.dtd">
<struts>
	<package name="index" extends="default" namespace="/index" strict-method-invocation="false">
		<default-interceptor-ref name="ZebraDefaultStack"/>
		<action name="main/*" class="com.es.portal.index.IndexAction" method="{1}">
			<result name="success" type="debugDispatcherResult">/com/es/portal/index/IndexDefault.jsp</result>
			<result name="dashboard" type="debugDispatcherResult">/com/es/portal/index/IndexDashboard.jsp</result>
		</action>
		<action name="contact/*" class="com.es.portal.index.IndexAction" method="{1}">
			<result name="contactus" type="debugDispatcherResult">/com/es/portal/index/ContactUs.jsp</result>
			<result name="contactuspop" type="debugDispatcherResult">/com/es/portal/index/ContactUsPop.jsp</result>
		</action>
	</package>
</struts>